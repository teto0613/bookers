<h1>Books</h1>

 <nav>
    <ul class="nav-list">
     <li class="nav-list-item">Title</li>
     <li class="nav-list-item">Body</li>
    </ul>
 </nav>

<h2>New Book</h2>

<%= form_with model: @book do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
<% end %>